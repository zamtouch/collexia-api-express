    "mandateRequest": {
      "type": "object",
      "properties": {
        "clientNo": {
          "type": "string",
          "minLength": 0,
          "maxLength": 15,
          "description": "Alphanumeric number for your client"
        },
        "userReference": {
          "type": "string",
          "minLength": 1,
          "maxLength": 10,
          "description": "Alphanumeric reference for the mandate"
        },
        "frequencyCode": {
          "type": "integer",
          "description": "Code for the frequency\n1-Weekly,3-Fortnightly,4-Monthly, 5-Monthly by Rule\n"
        },
        "installmentAmount": {
          "type": "number",
          "description": "Installment amount in Currency"
        },
        "noOfInstallments": {
          "type": "integer",
          "description": "Number of installments "
        },
        "origin": {
          "type": "integer",
          "description": "Origin will be supplied"
        },
        "contractReference": {
          "type": "string",
          "description": "First 4 characters will be the Merchant’s GID in HEX.\nNext 4 character is the calculated base date\nLast 6 characters is the teller for the day\n",
          "minLength": 14,
          "maxLength": 14
        },
        "magId": {
          "type": "integer",
          "description": "20 - TMCT\n21 - MCT\n45 - DC\n46 - Endo\n54 - RM\n"
        },
        "initialAmount": {
          "type": "number",
          "description": "The amount you want to collect on your first installment.(Will override the installmentAmount for the 1st installment)\n\nOnly to be used when you want to collect a greater amount than the installmentAmount.\n\nLeave 0 if not used.\n"
        },
        "firstCollectionDate": {
          "type": "string",
          "description": "The first installment date",
          "minLength": 8,
          "maxLength": 8,
          "format": "yyyymmdd"
        },
        "collectionDay": {
          "type": "integer",
          "description": "Day of the Month/Week you want to collect \nWeekly \n—-------------\n01 - Monday\n02 - Tuesday\n03 - Wednesday\n04 - Thursday\n05 - Friday\n06 - Saturday\n07 - Sunday\n\nFortnightly\n—------------------\n01 Monday to 07 Sunday  of 1st week\n08 Monday to 14 Sunday of 2nd week\n\nMonthly\n—-------------\n1-30\n31/99 = Last day of the month\n"
        },
        "numberOfTrackingDays": {
          "type": "integer",
          "description": "No of Tracking days 1-10"
        },
        "debtorAccountName": {
          "type": "string",
          "description": "Name of Debtor",
          "minLength": 35,
          "maxLength": 35
        },
        "debtorIdentificationType": {
          "type": "integer",
          "description": "Identification type of the Debtor.\n\n1 - RSA ID\n2 - Passport\n3 - Temp ID\n4- Business\n"
        },
        "debtorIdentificationNo": {
          "type": "string",
          "description": "Identification number of Debtor.",
          "minLength": 33,
          "maxLength": 33
        },
        "debtorAccountNumber": {
          "type": "string",
          "description": "Account number of the Debtor",
          "minLength": 1,
          "maxLength": 9
        },
        "debtorAccountType": {
          "type": "integer",
          "description": "Account type of the Debtor account.\n\n1 - Current\n2 - Savings\n3 - Transmission\n"
        },
        "debtorBanId": {
          "type": "integer",
          "description": "Debtor Bank Id"
        }
      },
      "required": [
        "userReference",
        "frequencyCode",
        "installmentAmount",
        "noOfInstallments",
        "origin",
        "contractReference",
        "magId",
        "collectionDay",
        "numberOfTrackingDays",
        "debtorAccountName",
        "debtorIdentificationType",
        "debtorIdentificationNo",
        "debtorBanId",
        "debtorAccountType",
        "debtorAccountNumber"
      ]
    },
    "messageInfo": {
      "type": "object",
      "properties": {
        "merchantGid": {
          "type": "integer",
          "description": "The Merchant’s GID as provided"
        },
        "remoteGid": {
          "type": "integer",
          "description": "System ID as provided"
        },
        "messageDate": {
          "type": "string",
          "minLength": 8,
          "maxLength": 8,
          "format": "yyyymmdd",
          "description": "Date the message was sent"
        },
        "messageTime": {
          "type": "string",
          "minLength": 6,
          "maxLength": 6,
          "format": "hhmmss",
          "description": "Time the message was sent"
        },
        "systemUserName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30,
          "description": "Your integration username that will be supplied to you, this will always be the same"
        },
        "frontEndUserName": {
          "type": "string",
          "minLength": 1,
          "maxLength": 30,
          "description": "This is the username of the user on your system that is loading the mandate."
        }
      },
      "required": [
        "merchantGid",
        "remoteGid",
        "messageDate",
        "messageTime",
        "systemUserName",
        "frontEndUserName"
      ]
    },
