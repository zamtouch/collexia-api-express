              },
              "message": {
                "type": "string",
                "description": "Error Text",
                "minLength": 1,
                "maxLength": 250
              },
              "detail": {
                "type": "string",
                "description": "Detail of the Error",
                "minLength": 1,
                "maxLength": 50
              },
              "rcoId": {
                "type": "integer"
              }
            },
            "required": [
