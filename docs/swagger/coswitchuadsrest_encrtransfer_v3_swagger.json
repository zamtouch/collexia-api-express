        },
        "errors": {
          "type": "array",
          "description": "ErrorCodes ",
          "items": {
            "type": "object",
            "properties": {
              "code": {
                "type": "string",
                "description": "Error Code (1.8 Error Codes)"
              },
              "message": {
                "type": "string",
                "description": "Error Text"
              },
              "detail": {
                "type": "string",
                "description": "Detail of the Error"
              }
            },
